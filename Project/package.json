{
	"name":    "@g00br0ldb3rg/midi-starter",
	"version": "1.0.0",
	"type":    "module",
	"license": "MIT",

	"scripts": {
		"dev":   "vite          --config ./__Configuration__/Vite/index.ts",
		"build": "vite build    --config ./__Configuration__/Vite/index.ts",
		"start": "vite preview  --config ./__Configuration__/Vite/index.ts",

		"repl":       "cls &&                                                             node  --loader ts-node/esm  --require tsconfig-paths/register  ./src/__REPL__.ts",
		"repl-watch": "nodemon  --config ./__Configuration__/NodeMon/Config.json  --exec  node  --loader ts-node/esm  --require tsconfig-paths/register  ./src/__REPL__.ts"
	},

	"dependencies": {
		"lodash-es":                        "4.17.21",
		"@types/lodash-es":                 "4.17.6",

		"solid-js":                         "1.6.9",
		"solid-use":                        "0.6.2",
		"solid-styled-components":          "0.28.5",
		"@solid-primitives/event-listener": "2.2.5",
		"@solid-primitives/scheduled":      "1.2.1",
		"@solid-primitives/timer":          "1.3.5",

		"@hope-ui/solid":                   "0.6.7",
		"@stitches/core":                   "1.2.8",
		"solid-transition-group":           "0.0.12",

		"@fontsource/exo-2":                "4.5.11",

		"xstate":                           "4.35.2",
		"@xstate/solid":                    "file:../Packages/xstate-solid/",

		"tone":                             "14.7.77",
		"startaudiocontext":                "1.2.1"
	},

	"devDependencies": {
		"@types/node":         "18.11.18",
		"@types/webmidi":      "2.0.6",

		"nodemon":             "2.0.20",

		"typescript":          "4.9.4",
		"tsconfig-paths":      "4.1.2",
		"ts-node":             "10.9.1",
		"ts-toolbelt":         "9.6.0",

		"vite":                "4.0.4",
		"vite-plugin-solid":   "2.5.0",
		"vite-tsconfig-paths": "4.0.5",

		"sass":                "1.57.1",

		"unplugin-icons":      "0.15.1",
		"@iconify/json":       "2.2.9"
	}

}
